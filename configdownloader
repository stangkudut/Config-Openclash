echo -e "----- [ OPENCLASH CONFIG DOWNLOADER ] -----"
sleep 5
echo -e "----- [ SEDANG MENDOWNLOAD CONFIG ] -----"
cd /etc/openclash/config
wget -q -O ST4NGKUDUT.yaml "https://raw.githubusercontent.com/stangkudut/Config-Openclash/main/config/ST4NGKUDUT.yaml"
clear
